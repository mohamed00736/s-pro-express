<script>
jQuery(document).ready(function($) {

	// Moving sidebar
	$(document).scroll(function() {	 	
		
		var p_h = $('#page_content').height(); // sidebar height
		var s_h = $('.sidebar').height(); // sidebar height
		
		if ($(document).scrollTop() >= 350) {
			
			var scrollable = p_h - s_h; // scrollable px
			var scrolled = $(document).scrollTop() - 350; // what was scrolled
			if(scrolled < scrollable) {
				$('.sidebar').css('top',scrolled+'px');
			}		
		}
		else {
			$('.sidebar').css('top','0');
		}
	});

});
</script>

